const division = () => {
    const number = process.argv;
    number.splice (0,2);

    
    if(
        number.length>2 ||
        number.length<2 ||
        number[1] == 0 ||
        number[1] >= number[0] ||
        isNaN(number[0]) ||
        isNaN(number[1])
    ){
        return "error";
    }

    return `rÃ©sultat: ${Math.floor(number[0] / number[1])}
    reste: ${number[0] % number[1]}`;
};

console.log(division());
